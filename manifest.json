{
    "name": "paywall Remover",
    "version": "1.0",
    "description": "去除付费墙",
    "manifest_version": 2,
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "webNavigation",
        "webRequest",
        "cookies",
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon-default16.png",
            "32": "images/icon-default32.png",
            "48": "images/icon-default48.png",
            "128": "images/icon-default128.png"
        }
    },
    "icons": {
        "16": "images/icon-default16.png",
        "32": "images/icon-default32.png",
        "48": "images/icon-default48.png",
        "128": "images/icon-default128.png"
    }
}